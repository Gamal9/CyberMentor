<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CyberMentor.View.MainMdPage.MenuPage" NavigationPage.HasNavigationBar="False"
             xmlns:pages="clr-namespace:CyberMentor.View.MainMdPage"
                  xmlns:cybermentor="clr-namespace:CyberMentor">
    
  <MasterDetailPage.Master>
    <pages:MenuPageMaster x:Name="MasterPage" />
  </MasterDetailPage.Master>
  <MasterDetailPage.Detail>
        <NavigationPage FlowDirection="MatchParent" NavigationPage.HasNavigationBar="False">
            <x:Arguments>
                <cybermentor:MainPage NavigationPage.HasNavigationBar="False"/>
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>